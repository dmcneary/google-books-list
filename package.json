{
  "name": "google-books-list",
  "version": "1.0.0",
  "description": "A Google Books search tool built on the MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "David McNeary",
  "repository": {
    "type": "git",
    "url": "git@github.com:dmcneary/google-books-list.git"
  },
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.5"
  }
}
